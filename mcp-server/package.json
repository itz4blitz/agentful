{
  "name": "@itz4blitz/agentful-mcp-server",
  "version": "2.0.0-beta.1",
  "description": "MCP server for agentful pattern learning with vector database",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "agentful-mcp-server": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc && cp node_modules/sql.js/dist/sql-wasm.wasm dist/infrastructure/",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "dev": "npm run build && node dist/index.js",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "mcp",
    "vector-database",
    "agentful",
    "pattern-learning"
  ],
  "author": "agentful",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.6.0",
    "@xenova/transformers": "^2.17.2",
    "sql.js": "^1.12.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/sql.js": "^1.4.9",
    "@vitest/coverage-v8": "^2.1.8",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
