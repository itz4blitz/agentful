{
  "name": "@itz4blitz/agentful",
  "version": "0.5.0",
  "description": "Human-in-the-loop development kit for Claude Code with smart product analysis and natural conversation",
  "type": "module",
  "bin": {
    "agentful": "bin/cli.js"
  },
  "scripts": {
    "init": "node bin/cli.js init",
    "test": "vitest run",
    "test:unit": "vitest run test/unit",
    "test:validation": "vitest run test/validation",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "release": "semantic-release",
    "release:dry-run": "semantic-release --dry-run",
    "docs:dev": "vocs dev",
    "docs:build": "vocs build",
    "docs:preview": "vocs preview"
  },
  "exports": {
    ".": {
      "import": "./lib/index.js"
    }
  },
  "files": [
    "bin/",
    "lib/",
    "template/",
    ".claude/",
    "version.json",
    "LICENSE"
  ],
  "keywords": [
    "claude-code",
    "human-in-the-loop",
    "ai-development",
    "agent",
    "productivity"
  ],
  "author": "agentful",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itz4blitz/agentful.git"
  },
  "homepage": "https://agentful.app",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^13.1.3",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@vitest/coverage-v8": "^1.2.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^3.0.1",
    "eslint": "^9.39.2",
    "js-yaml": "^4.1.0",
    "semantic-release": "^25.0.2",
    "vitest": "^1.2.0",
    "vocs": "^1.4.1"
  }
}
