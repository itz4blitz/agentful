{
  "name": "@itz4blitz/agentful",
  "version": "1.8.2",
  "description": "Pre-configured AI toolkit with self-hosted execution - works with any LLM, any tech stack, any platform.",
  "type": "module",
  "bin": {
    "agentful": "bin/cli.js"
  },
  "scripts": {
    "init": "node bin/cli.js init",
    "test": "vitest run",
    "test:unit": "vitest run test/unit",
    "test:validation": "vitest run test/validation",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "release": "semantic-release",
    "release:dry-run": "semantic-release --dry-run",
    "docs:dev": "vocs dev --host 127.0.0.1 --port 8080",
    "docs:build": "vocs build",
    "docs:preview": "vocs preview",
    "build:all": "npm run build:mcp",
    "build:mcp": "cd packages/mcp-server && npm run build",
    "test:mcp": "cd packages/mcp-server && npm run test",
    "dev:mcp": "cd packages/mcp-server && npm run dev",
    "install:all": "npm install && cd packages/mcp-server && npm install"
  },
  "exports": {
    ".": {
      "import": "./lib/index.js"
    }
  },
  "files": [
    "bin/",
    "lib/",
    "template/",
    "version.json",
    "LICENSE"
  ],
  "keywords": [
    "claude-code",
    "self-hosted",
    "agent-server",
    "autonomous-development",
    "code-generation",
    "multi-platform",
    "ai-agents",
    "development-automation",
    "specialized-agents",
    "orchestrator",
    "ci-cd",
    "remote-execution"
  ],
  "author": "agentful",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itz4blitz/agentful.git"
  },
  "homepage": "https://agentful.app",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^13.1.3",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@vitest/coverage-v8": "^1.2.0",
    "eslint": "^9.39.2",
    "semantic-release": "^25.0.2",
    "vite-plugin-pagefind": "^1.0.7",
    "vitest": "^1.2.0",
    "vocs": "^1.4.1"
  },
  "dependencies": {
    "handlebars": "^4.7.8",
    "js-yaml": "^4.1.0",
    "qrcode.react": "^4.2.0"
  }
}
