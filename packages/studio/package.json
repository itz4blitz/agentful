{
  "name": "agentful-studio",
  "displayName": "agentful Studio",
  "description": "AI-powered visual development environment for agentful",
  "private": true,
  "version": "0.0.1",
  "publisher": "agentful",
  "repository": {
    "type": "git",
    "url": "https://github.com/itz4blitz/agentful"
  },
  "main": "./dist/extension.js",
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "keybindings": [
      {
        "command": "agentful.studio.reloadWebview",
        "key": "ctrl+shift+r",
        "mac": "cmd+shift+r",
        "when": "agentful.studio.active"
      }
    ],
    "commands": [
      {
        "command": "agentful.studio.open",
        "title": "Open agentful studio",
        "category": "agentful"
      },
      {
        "command": "agentful.studio.openIntegrationHub",
        "title": "Open Integration Hub",
        "category": "agentful"
      },
      {
        "command": "agentful.studio.reloadWebview",
        "title": "Reload Webview",
        "category": "agentful",
        "icon": "$(refresh)"
      },
      {
        "command": "agentful.studio.rescanComponents",
        "title": "Rescan Components",
        "category": "agentful"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "agentful-studio-sidebar",
          "title": "agentful Studio",
          "icon": "assets/agentful-icon.svg"
        }
      ]
    },
    "views": {
      "agentful-studio-sidebar": [
        {
          "type": "webview",
          "id": "agentful.studio.sidebar",
          "name": "Studio",
          "contextualTitle": "agentful Studio"
        }
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "dev:full": "npm run build && npm run dev:extension",
    "dev:extension": "code --extensionDevelopmentPath=${PWD}",
    "watch": "npm run watch:extension & npm run watch:webview",
    "watch:extension": "tsc -b tsconfig.extension.json --watch",
    "watch:webview": "vite build --watch --mode development",
    "watch:dev": "npm run build:extension && npm run watch:webview",
    "build": "npm run build:extension && npm run build:webview",
    "build:webview": "vite build",
    "build:extension": "tsc -b tsconfig.extension.json",
    "build:analyze": "tsc -b && vite build --mode analyze",
    "package": "vsce package",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "ci": "npm run typecheck && npm run lint && npm run test:run && npm run test:e2e && npm run build",
    "security": "npm audit && npm outdated"
  },
  "dependencies": {
    "@ai-sdk/react": "^1.2.4",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.2",
    "@itz4blitz/agentful": "^1.5.1",
    "@iarna/toml": "^2.2.5",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.8",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@testing-library/dom": "^10.4.1",
    "ai": "^4.3.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "immer": "^11.1.3",
    "input-otp": "^1.4.2",
    "jszip": "^3.10.1",
    "lucide-react": "^0.563.0",
    "next-themes": "^0.4.6",
    "react": "^19.2.0",
    "react-day-picker": "^9.13.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.71.1",
    "react-markdown": "^9.0.1",
    "react-resizable-panels": "^4.5.2",
    "react-router-dom": "^7.13.0",
    "react-syntax-highlighter": "^15.6.1",
    "recharts": "^2.15.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "zod": "^4.3.6",
    "zustand": "^5.0.10"
  },
  "engines": {
    "vscode": "^1.96.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@clerk/backend": "^2.29.6",
    "@eslint/js": "^9.39.1",
    "@faker-js/faker": "^10.2.0",
    "@playwright/test": "^1.58.0",
    "@t3-oss/env-core": "^0.13.10",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/jszip": "^3.4.0",
    "@types/node": "^24.10.9",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/vscode": "^1.96.0",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "@vscode/vsce": "^3.7.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-eslint-plugin": "^7.3.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-stylistic": "^0.0.0-0",
    "globals": "^16.5.0",
    "happy-dom": "^20.3.9",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^4.0.18"
  }
}
