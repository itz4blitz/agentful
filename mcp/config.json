{
  "server": {
    "name": "agentful-mcp",
    "version": "1.0.0",
    "description": "Model Context Protocol server for agentful autonomous development framework",
    "vendor": "agentful",
    "capabilities": {
      "tools": true,
      "resources": true,
      "prompts": true,
      "logging": true
    }
  },
  "agentful": {
    "rootDir": "..",
    "agentsDir": "../.claude/agents",
    "productDir": "../.claude/product",
    "stateDir": "../.agentful",
    "pipelineDir": "../lib/pipeline",
    "executorPath": "../lib/pipeline/executor.js"
  },
  "tools": {
    "enabled": [
      "execute-agent",
      "list-agents",
      "create-pipeline",
      "get-status",
      "validate-code",
      "analyze-product"
    ]
  },
  "resources": {
    "enabled": [
      "agent-definitions",
      "product-spec",
      "pipeline-state",
      "architecture"
    ],
    "cache": {
      "enabled": true,
      "ttl": 60000
    }
  },
  "logging": {
    "level": "info",
    "destination": "stderr"
  },
  "session": {
    "timeout": 3600000,
    "maxConcurrentExecutions": 5
  }
}
